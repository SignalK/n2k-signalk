  "license": "Apache-2.0",
  "repository": "https://github.com/SignalK/n2k-signalk",
  "dependencies": {
    "JSONStream": "~1.3.0",
    "debug": "^4.0.0",
    "lodash": "^4.17.11",
    "through": ">=2.2.7 <3"
  },
  "devDependencies": {
    "@signalk/github-create-release": "^1.0.1",
    "@signalk/signalk-schema": "1.3.1",
    "chai": "~4.1.0",
    "chai-things": "0.2",
    "mocha": "^7.1.0",
    "prettier-standard": "^16.0.0",
    "tv4": "latest"
  }
